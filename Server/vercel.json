{
    "version": 2,
    "builds": [
      {
        "src": "index.js",
        "use": "@vercel/node"
      }
    ],
    "routes": [
      {
        "src": "/api/(.*)",
        "dest": "/index.js"
      },
      {
        "src": "/(.*)",
        "dest": "/index.js"
      }
    ],
    "functions": {
      "index.js": {
        "runtime": "nodejs16.x",
        "maxDuration": 10,
        "memory": 1024
      }
    }
  }
  